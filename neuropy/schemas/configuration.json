{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "load_from",
    "save_to",
    "eval_metric",
    "keep_checkpoint_max",
    "log_step_count_steps",
    "save_summary_steps"
  ],
  "properties": {
    "load_from": {
      "$id": "#/properties/load_from",
      "type": "string",
      "title": "The Load_from Schema",
      "default": "",
      "examples": [
        "default"
      ],
      "pattern": "^(.*)$"
    },
    "save_to": {
      "$id": "#/properties/save_to",
      "type": "string",
      "title": "The Save_to Schema",
      "default": "",
      "examples": [
        "default"
      ],
      "pattern": "^(.*)$"
    },
    "eval_metric": {
      "$id": "#/properties/eval_metric",
      "type": "string",
      "title": "The eval_metric Schema",
      "default": "",
      "examples": [
        "val_loss",
        "val_acc"
      ],
      "pattern": "^(.*)$"
    },
    "save_best": {
      "$id": "#/properties/save_best",
      "type": "boolean",
      "title": "The save_best Schema",
      "default": true,
      "examples": [
        "val_loss",
        "val_max"
      ],
      "pattern": "^(.*)$"
    },
    "keep_checkpoint_max": {
      "$id": "#/properties/keep_checkpoint_max",
      "type": "integer",
      "title": "The Keep_checkpoint_max Schema",
      "default": 5,
      "examples": [
        5
      ]
    },
    "log_step_count_steps": {
      "$id": "#/properties/log_step_count_steps",
      "type": "integer",
      "title": "The Log_step_count_steps Schema",
      "default": 100,
      "examples": [
        100
      ]
    },
    "save_summary_steps": {
      "$id": "#/properties/save_summary_steps",
      "type": "integer",
      "title": "The Save_summary_steps Schema",
      "default": 100,
      "examples": [
        100
      ]
    }
  }
}